<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.stylefeng.guns.rest.common.persistence.dao.MtimeHallDictTMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.stylefeng.guns.rest.common.persistence.model.MtimeHallDictT">
        <id column="UUID" property="uuid" />
        <result column="show_name" property="showName" />
        <result column="seat_address" property="seatAddress" />
    </resultMap>
    <select id="selectAllHand" resultType="com.stylefeng.guns.rest.common.persistence.vo.dhd.Hall">
        select UUID,show_name,seat_address from mtime_hall_dict_t
    </select>
    <select id="selectByHallId" resultType="com.stylefeng.guns.rest.common.persistence.model.MtimeHallDictT">
        select UUID,show_name as showName,seat_address from  mtime_hall_dict_t where UUID = #{hallId}
    </select>

</mapper>
